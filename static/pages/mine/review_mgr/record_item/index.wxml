<view wx:for="{{listData}}" wx:key="{{item.id}}" class='item'>
  <view class='top'>
    <view class='top-left'>
      <image class='img' mode='aspectFill' src='{{item.headPic?item.headPic:"/images/ic_mine_select.png"}}'></image>
      <view class='center'>
        <view class='name'>{{item.realName}} 
          <text  class='tag' style="background:#FFBB00;" data-userid="{{item.userId}}" wx:if="{{item.entryStatus === 300}}" bindtap="_btnTap">待审核</text>
          <text  class='tag' style="background:#00CCCC;" wx:if="{{item.entryStatus === 400}}">通过</text> 
          <text  class='tag' style="background:#F60554;" wx:if="{{item.entryStatus === 500}}">不通过</text> 
        </view>
        <view class=''>{{item.companyName}}</view>
        <view class=''>{{item.jobName}}</view>
      </view>
    </view>
    <view class='top-right'>
      <view class='tel-box' data-tel='{{item.phone}}' bindtap='_totel'>
        <image src='/images/tel.png'></image>
        <view>联系</view>
      </view>
      <view>{{item.interviewTime}}</view>
    </view>
  </view>

</view>