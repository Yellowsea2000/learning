page{
  height: 100%;
  background: #fff;
  position: relative;
}
.wrap{
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items:center;
}
.contact{
  margin: 0 40rpx 200rpx 40rpx;
}
.btn-box{
