
<loading showLoading="{{showLoading}}"></loading>
<view class='wrap' wx:if="{{entryStatus == 520}}">
  <view class='contact'>
    <import src="/utils/wxParse/wxParse.wxml" />
    <template is="wxParse" data="{{wxParseData:contactTemp.nodes}}" />
  </view>
  <view class='btn-box'>
    <button class='bottom-btn' bindtap='startSign'>开始签订</button>
  </view>
</view>
<web-view src="{{contactUrl}}" wx:if="{{entryStatus == 530}}"></web-view>